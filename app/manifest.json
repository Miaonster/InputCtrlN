{
    "name": "__MSG_appName__",
    "version": "0.1.5",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "images/icon-16.png",
        "50": "images/icon-50.png",
        "100": "images/icon-100.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "scripts/main.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "scripts/script.js"
    ]
}